{
  "name": "webserver",
  "version": "1.0.0",
  "description": "A simple node proxy server for pica",
  "scripts": {
    "serve": "npx ts-node app.ts",
    "dev": "npx nodemon --exec \"npm run\" serve --config nodemon.config.json",
    "start": "npx pm2 start npm --name picaServerProd --env production -- run serve",
    "stop": "npx pm2 delete picaServerProd"
  },
  "author": "拆家大主教",
  "license": "MIT",
  "dependencies": {
    "@stdlib/assert-is-json": "^0.0.7",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "download": "^8.0.0",
    "express": "^4.17.1",
    "jszip": "^3.7.1",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/download": "^8.0.1",
    "@types/express": "^4.17.13",
    "@types/leveldown": "^4.0.3",
    "@types/levelup": "^4.3.3",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.1",
    "nodemon": "^2.0.15",
    "pm2": "^5.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
